{
  "name": "bankaapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run clean-db && nyc --reporter=text mocha server/api/__test__/*.js --require @babel/register --exit",
    "start": "node ./build/app.js",
    "dev": "set DEBUG= dev && nodemon --exec babel-node server/app.js",
    "build": "rm -rf build && mkdir build && babel -d ./build ./server",
    "heroku-postbuild": "npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "clean-db": "set DEBUG= dev && babel-node server/api/database/dropTables && babel-node server/api/database/createTables.js && babel-node server/api/database/seed.js"
  },

  "keywords": [
    "banka",
    "node",
    "express",
    "api",
    "endpoints"
  ],
 
  "homepage": "https://github.com/tmosco/bankaApp#readme",
  "dependencies": {
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "morgan": "^1.9.1",
    "pg": "^7.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.0.0"
  }
}
